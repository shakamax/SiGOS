<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ordemServicoServlet</servlet-name>
        <servlet-class>br.com.sigos.servlet.ordemServicoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServicoServlet</servlet-name>
        <servlet-class>br.com.sigos.servlet.ServicoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClienteServlet</servlet-name>
        <servlet-class>br.com.sigos.servlet.ClienteServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FuncionarioServlet</servlet-name>
        <servlet-class>br.com.sigos.servlet.FuncionarioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProdutoServlet</servlet-name>
        <servlet-class>br.com.sigos.servlet.ProdutoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EquipamentosServlet</servlet-name>
        <servlet-class>br.com.sigos.servlet.EquipamentosServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>br.com.sigos.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AreaClienteServlet</servlet-name>
        <servlet-class>br.com.sigos.servlet.AreaClienteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ordemServicoServlet</servlet-name>
        <url-pattern>/ordemServicoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServicoServlet</servlet-name>
        <url-pattern>/ServicoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClienteServlet</servlet-name>
        <url-pattern>/ClienteServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FuncionarioServlet</servlet-name>
        <url-pattern>/FuncionarioServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProdutoServlet</servlet-name>
        <url-pattern>/ProdutoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EquipamentosServlet</servlet-name>
        <url-pattern>/EquipamentosServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AreaClienteServlet</servlet-name>
        <url-pattern>/AreaClienteServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>15000</session-timeout>
    </session-config>
</web-app>
